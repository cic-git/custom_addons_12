<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for Maintenance requests -->
        <record id="cicon_equipment.equip_seq" model="ir.sequence">
            <field name="name">CICON Equipment Ref</field>
            <field name="code">cicon.equip.internal.seq</field>
            <field name="prefix">CE/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>